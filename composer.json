{
    "name": "MODXRenderer",
    "description": "The MODX Revolution parser unit tested and refactored for use with Slim or other frameworks.",
    "keywords": ["MODX", "template", "Slim"],
    "homepage": "https://github.com/sepiariver/modxrenderer",
    "license": "GPL",
    "authors": [{
        "name": "YJ Tso",
        "email": "info@sepiariver.com",
        "homepage": "https://www.sepiariver.ca/"
    }],
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0",
        "slim/slim": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "SepiaRiver\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    }

}
